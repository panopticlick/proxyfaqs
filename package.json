{
  "name": "proxyfaqs",
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "import:paa": "bun run scripts/import-paa.ts",
    "import:providers": "bun run scripts/import-providers.ts",
    "import:keywords": "bun run scripts/import-keywords.ts",
    "import:articles": "bun run scripts/import-articles.ts",
    "import:all": "bun run import:paa && bun run import:providers && bun run import:keywords",
    "sitemap": "bun run scripts/build-sitemap.ts",
    "test": "bun test",
    "test:unit": "bun test --only",
    "typecheck": "astro check",
    "deploy": "wrangler pages deploy dist --project-name proxyfaqs",
    "deploy:preview": "wrangler pages deploy dist --project-name proxyfaqs --branch preview",
    "security-check": "bash scripts/security-check.sh",
    "setup:github": "bash scripts/setup-github.sh",
    "precommit": "bun run security-check",
    "lint": "eslint src",
    "format": "prettier --write \"src/**/*.{astro,ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{astro,ts,tsx}\""
  },
  "dependencies": {
    "@astrojs/check": "^0.9.0",
    "@astrojs/cloudflare": "12.6.12",
    "@astrojs/sitemap": "^3.2.0",
    "@astrojs/tailwind": "^5.1.0",
    "@supabase/supabase-js": "^2.47.0",
    "@types/pg": "8.16.0",
    "astro": "^4.16.0",
    "marked": "^15.0.0",
    "papaparse": "5.5.3",
    "pg": "8.16.3",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@tailwindcss/typography": "0.5.19",
    "@types/bun": "^1.1.0",
    "@types/libsodium-wrappers": "0.7.14",
    "@types/papaparse": "5.5.2",
    "eslint": "^9.0.0",
    "eslint-plugin-astro": "^1.2.0",
    "libsodium-wrappers": "0.8.0",
    "libsodium-wrappers-sumo": "0.8.0",
    "prettier": "^3.2.0",
    "prettier-plugin-astro": "^0.14.0",
    "tailwindcss": "^3.4.0",
    "tweetnacl": "1.0.3",
    "tweetnacl-util": "0.15.1",
    "typescript": "^5.6.0",
    "typescript-eslint": "^8.0.0"
  }
}
